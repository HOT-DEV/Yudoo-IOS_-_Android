    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/fifty"
        android:background="@color/whiteColor"
        android:gravity="center">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/poppins_bold"
            android:text="@string/whatislooking"
            android:textColor="@color/regEmailColor"
            android:textSize="@dimen/twenty" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/fourty"
        android:layout_marginLeft="@dimen/twenty"
        android:layout_marginRight="@dimen/twenty"
        android:background="@color/whiteColor"
        android:orientation="horizontal">


        <Button
            android:id="@+id/btnfriend"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:layout_margin="@dimen/five"
            android:layout_weight="0.5"
            android:background="@drawable/circleimg"
            android:backgroundTint="@color/literose"
            android:fontFamily="@font/poppins_bold"
            android:shadowColor="@color/whiteLightColor"
            android:text="@string/friends"
            android:textAllCaps="false"
            android:textColor="@color/rose"
            android:textSize="@dimen/sixteen"
            tools:ignore="Suspicious0dp" />


        <Button
            android:id="@+id/btndating"
            android:layout_width="10dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:layout_weight="0.5"
            android:layout_margin="@dimen/five"
            android:background="@drawable/circleimg"
            android:backgroundTint="@color/literose"
            android:fontFamily="@font/poppins_bold"
            android:shadowColor="@color/whiteLightColor"
            android:text="@string/date"
            android:textAllCaps="false"
            android:textColor="@color/rose"
            android:textSize="@dimen/sixteen"
            tools:ignore="Suspicious0dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/ten"
        android:layout_marginLeft="@dimen/twenty"
        android:layout_marginRight="@dimen/twenty"
        android:background="@color/whiteColor"
        android:orientation="horizontal">


        <Button
            android:id="@+id/btnrelation"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:layout_margin="@dimen/five"
            android:paddingLeft="@dimen/ten"
            android:paddingRight="@dimen/ten"
            android:layout_weight="0.5"
            android:background="@drawable/circleimg"
            android:backgroundTint="@color/literose"
            android:fontFamily="@font/poppins_bold"
            android:shadowColor="@color/whiteLightColor"
            android:text="@string/marriage"
            android:textAllCaps="false"
            android:textColor="@color/rose"
            android:textSize="@dimen/sixteen"
            tools:ignore="Suspicious0dp" />


        <Button
            android:id="@+id/btntravel"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:layout_margin="@dimen/five"
            android:layout_weight="0.5"
            android:background="@drawable/circleimg"
            android:backgroundTint="@color/literose"
            android:fontFamily="@font/poppins_bold"
            android:shadowColor="@color/whiteLightColor"
            android:text=""
            android:textAllCaps="false"
            android:textColor="@color/rose"
            android:textSize="@dimen/sixteen"
            android:visibility="invisible"
            tools:ignore="Suspicious0dp" />

    </LinearLayout>

